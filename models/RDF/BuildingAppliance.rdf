    <!-- http://bemserver.org/appliance#Wearable -->

    <owl:Class rdf:about="http://bemserver.org/appliance#Wearable">
        <rdfs:subClassOf rdf:resource="http://bemserver.org/appliance##MobileBuildingAppliance"/>
        <rdfs:comment xml:lang="en">Wearable is a building appliance representing a wereable device such as smartwatches and smartphones.</rdfs:comment>
        <rdfs:label xml:lang="en">Wearable</rdfs:label>
    </owl:Class>



    <!-- http://bemserver.org/appliance##Air -->

    <owl:Class rdf:about="http://bemserver.org/appliance##Air">
        <rdfs:subClassOf rdf:resource="http://bemserver.org/appliance##PhysicalMedium"/>
        <rdfs:comment xml:lang="en">Air can be consumed or produced for an energy generation process.</rdfs:comment>
        <rdfs:label xml:lang="en">Air</rdfs:label>
    </owl:Class>



    <!-- http://bemserver.org/appliance##BuildingAppliance -->

    <owl:Class rdf:about="http://bemserver.org/appliance##BuildingAppliance">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://bemserver.org/appliance##contains"/>
                <owl:someValuesFrom rdf:resource="http://bemserver.org/appliance##BuildingAppliance"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Building Appliances represent the static and mobile appliances inside the building. Several devices and IFC specific devices has been categorised and included.</rdfs:comment>
        <rdfs:label xml:lang="en">BuildingAppliance</rdfs:label>
    </owl:Class>



    <!-- http://bemserver.org/appliance##Gas -->

    <owl:Class rdf:about="http://bemserver.org/appliance##Gas">
        <rdfs:subClassOf rdf:resource="http://bemserver.org/appliance##PhysicalMedium"/>
        <rdfs:comment xml:lang="en">Gas can be consumed or produced for an energy generation process.</rdfs:comment>
        <rdfs:label xml:lang="en">Gas</rdfs:label>
    </owl:Class>



    <!-- http://bemserver.org/appliance##Meter -->

    <owl:Class rdf:about="http://bemserver.org/appliance##Meter">
        <rdfs:subClassOf rdf:resource="http://bemserver.org/appliance##Monitoring"/>
        <rdfs:comment xml:lang="en">A Meter is a device which measures the integral, with respect to time, of the power in an electric circuit.</rdfs:comment>
        <rdfs:label xml:lang="en">Meter</rdfs:label>
    </owl:Class>



    <!-- http://bemserver.org/appliance##MobileBuildingAppliance -->

    <owl:Class rdf:about="http://bemserver.org/appliance##MobileBuildingAppliance">
        <rdfs:subClassOf rdf:resource="http://bemserver.org/appliance##BuildingAppliance"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://bemserver.org/appliance##wasLocated"/>
                <owl:allValuesFrom rdf:resource="http://bemserver.org/sensor#MobileBuildingApplianceLocationObservation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Mobile Building Appliances represent  mobile appliances inside the building.</rdfs:comment>
        <rdfs:label xml:lang="en">MobileBuildingAppliance</rdfs:label>
    </owl:Class>



    <!-- http://bemserver.org/appliance##Monitoring -->

    <owl:Class rdf:about="http://bemserver.org/appliance##Monitoring">
        <rdfs:subClassOf rdf:resource="http://bemserver.org/appliance##StaticBuildingAppliance"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://bemserver.org/appliance##MeasureInputOutput"/>
                <owl:someValuesFrom rdf:resource="http://bemserver.org/appliance##BuildingAppliance"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Monitoring is a static building appliance for monitoring and controlling the building, occupantsâ€™ devices, etc.</rdfs:comment>
        <rdfs:label xml:lang="en">Monitoring</rdfs:label>
    </owl:Class>



    <!-- http://bemserver.org/appliance##Oil -->

    <owl:Class rdf:about="http://bemserver.org/appliance##Oil">
        <rdfs:subClassOf rdf:resource="http://bemserver.org/appliance##PhysicalMedium"/>
        <rdfs:comment xml:lang="en">Oil can be consumed or produced for an energy generation process.</rdfs:comment>
        <rdfs:label xml:lang="en">Oil</rdfs:label>
    </owl:Class>



    <!-- http://bemserver.org/appliance##PhysicalMedium -->

    <owl:Class rdf:about="http://bemserver.org/appliance##PhysicalMedium">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/ssn/hasProperty"/>
                <owl:someValuesFrom rdf:resource="http://bemserver.org/property#PhenomenonProperty"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A Physical Medium represents the element consumed or produced for an energy generation process.</rdfs:comment>
        <rdfs:label xml:lang="en">PhysicalMedium</rdfs:label>
    </owl:Class>



    <!-- http://bemserver.org/appliance##Radiation -->

    <owl:Class rdf:about="http://bemserver.org/appliance##Radiation">
        <rdfs:subClassOf rdf:resource="http://bemserver.org/appliance##PhysicalMedium"/>
        <rdfs:comment xml:lang="en">Radiation can be consumed or produced for an energy generation process.</rdfs:comment>
        <rdfs:label xml:lang="en">Radiation</rdfs:label>
    </owl:Class>



    <!-- http://bemserver.org/appliance##StaticBuildingAppliance -->

    <owl:Class rdf:about="http://bemserver.org/appliance##StaticBuildingAppliance">
        <rdfs:subClassOf rdf:resource="http://bemserver.org/appliance##BuildingAppliance"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://bemserver.org/appliance##wasLocated"/>
                <owl:allValuesFrom rdf:resource="http://bemserver.org/sensor#MobileBuildingApplianceLocationObservation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Static Building Appliances represent static appliances inside the building. Several devices and IFC specific devices has been categorised and included.</rdfs:comment>
        <rdfs:label xml:lang="en">StaticBuildingAppliance</rdfs:label>
    </owl:Class>



    <!-- http://bemserver.org/appliance##Steam -->

    <owl:Class rdf:about="http://bemserver.org/appliance##Steam">
        <rdfs:subClassOf rdf:resource="http://bemserver.org/appliance##PhysicalMedium"/>
        <rdfs:comment xml:lang="en">Steam can be consumed or produced for an energy generation process.</rdfs:comment>
        <rdfs:label xml:lang="en">Steam</rdfs:label>
    </owl:Class>



    <!-- http://bemserver.org/appliance##Water -->

    <owl:Class rdf:about="http://bemserver.org/appliance##Water">
        <rdfs:subClassOf rdf:resource="http://bemserver.org/appliance##PhysicalMedium"/>
        <rdfs:comment xml:lang="en">Water can be consumed or produced for an energy generation process.</rdfs:comment>
        <rdfs:label xml:lang="en">Water</rdfs:label>
    </owl:Class>



    <!-- http://bemserver.org/property#PhenomenonProperty -->

    <owl:Class rdf:about="http://bemserver.org/property#PhenomenonProperty"/>



    <!-- http://bemserver.org/sensor#MobileBuildingApplianceLocationObservation -->

    <owl:Class rdf:about="http://bemserver.org/sensor#MobileBuildingApplianceLocationObservation">
        <rdfs:subClassOf rdf:resource="http://bemserver.org/sensor#Observation"/>
        <rdfs:comment xml:lang="en">This observation specifies a particular location of a mobile building appliance in a specific time.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">http://bemserver.org/sensor#MobileBuildingApplianceLocationObservation</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">MobileBuildingApplianceLocationObservation</rdfs:label>
    </owl:Class>



    <!-- http://bemserver.org/sensor#Observation -->

    <owl:Class rdf:about="http://bemserver.org/sensor#Observation">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://bemserver.org/appliance##&apos;external_ID&apos;"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Observation is an extension of ssn#Observation  defined in SSN. We have extended it and made them equivalents.
An Observation is a Situation in which a Sensing method has been used to estimate or calculate a value of a Property of a FeatureOfInterest.  Links to Sensing and Sensor describe what made the Observation and how; links to Property and Feature detail what was sensed; the result is the output of a Sensor; other metadata details times etc.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">http://bemserver.org/sensor#Observation</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Observation</rdfs:label>
    </owl:Class>



    <!-- http://hit2gap.eu/ontomg#BranchController -->

    <owl:Class rdf:about="http://hit2gap.eu/ontomg#BranchController">
        <rdfs:comment xml:lang="en">A Branch Controller is the manager of  branch operations. A branch is an energy consumer or producer that has a specific energy load over a certain time span.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">http://hit2gap.eu/ontomg#BranchController</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">BranchController</rdfs:label>
    </owl:Class>



    <!-- http://hit2gap.eu/ontomg#ELBranch -->

    <owl:Class rdf:about="http://hit2gap.eu/ontomg#ELBranch">
        <rdfs:comment xml:lang="en">An Electrical Load Branch is an electrical component or branch that consumes electric power. It mainly consists of electrical appliance components.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">http://hit2gap.eu/ontomg#ELBranch</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">ELBranch</rdfs:label>
    </owl:Class>



    <!-- http://hit2gap.eu/ontomg#ESBranch -->

    <owl:Class rdf:about="http://hit2gap.eu/ontomg#ESBranch">
        <rdfs:comment xml:lang="en">Energy storage systems play an essential role in stabilizing the power system, improving the quality of power supply, and achieving power peak shaving. The energy storage branch consists mainly of the energy storage device (e.g., flywheel, super-capacitor) and a converter.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">http://hit2gap.eu/ontomg#ESBranch</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">ESBranch</rdfs:label>
    </owl:Class>



    <!-- http://ifcowl.openbimstandards.org/IFC4_ADD2/index.html#IfcActuator -->

    <owl:Class rdf:about="http://ifcowl.openbimstandards.org/IFC4_ADD2/index.html#IfcActuator">
        <owl:equivalentClass rdf:resource="http://www.buildingsmart-tech.org/ifcOWL/IFC2X3_Final#IfcActuatorType"/>
        <rdfs:subClassOf rdf:resource="http://bemserver.org/appliance##Monitoring"/>
        <rdfs:comment xml:lang="en">An actuator is a mechanical device for moving or controlling a mechanism or system. An actuator takes energy, usually created by air, electricity, or liquid, and converts that into some kind of motion.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">http://ifcowl.openbimstandards.org/IFC4_ADD2/index.html#IfcActuator</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">IfcActuator</rdfs:label>
    </owl:Class>



    <!-- http://ifcowl.openbimstandards.org/IFC4_ADD2/index.html#IfcAlarm -->

    <owl:Class rdf:about="http://ifcowl.openbimstandards.org/IFC4_ADD2/index.html#IfcAlarm">
        <owl:equivalentClass rdf:resource="http://www.buildingsmart-tech.org/ifcOWL/IFC2X3_Final#IfcAlarmType"/>
        <rdfs:subClassOf rdf:resource="http://bemserver.org/appliance##Monitoring"/>
        <rdfs:comment xml:lang="en">An alarm is a device that signals the existence of a condition or situation that is outside the boundaries of normal expectation or that activates such a device. Alarms include the provision of break glass buttons and manual pull boxes that are used to activate alarms.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">http://ifcowl.openbimstandards.org/IFC4_ADD2/index.html#IfcAlarm</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">IfcAlarm</rdfs:label>
    </owl:Class>



    <!-- http://ifcowl.openbimstandards.org/IFC4_ADD2/index.html#IfcElectricFlowStorageDevice -->

    <owl:Class rdf:about="http://ifcowl.openbimstandards.org/IFC4_ADD2/index.html#IfcElectricFlowStorageDevice">
        <owl:equivalentClass rdf:resource="http://www.buildingsmart-tech.org/ifcOWL/IFC2X3_Final#IfcElectricFlowStorageDeviceType"/>
        <rdfs:subClassOf rdf:resource="http://hit2gap.eu/ontomg#ESBranch"/>
        <rdfs:subClassOf rdf:resource="http://www.buildingsmart-tech.org/ifcOWL/IFC2X3_Final#IfcFlowStorageDevice"/>
        <rdfs:comment xml:lang="en">An electric flow storage device is a device in which electrical energy is stored and from which energy may be progressively released.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">http://ifcowl.openbimstandards.org/IFC4_ADD2/index.html#IfcElectricFlowStorageDevice</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">IfcElectricFlowStorageDevice</rdfs:label>
    </owl:Class>



    <!-- http://ifcowl.openbimstandards.org/IFC4_ADD2/index.html#IfcSensor -->

    <owl:Class rdf:about="http://ifcowl.openbimstandards.org/IFC4_ADD2/index.html#IfcSensor">
        <owl:equivalentClass rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Sensor"/>
        <owl:equivalentClass rdf:resource="http://www.buildingsmart-tech.org/ifcOWL/IFC2X3_Final#IfcSensorType"/>
        <rdfs:subClassOf rdf:resource="http://bemserver.org/appliance##Monitoring"/>
        <rdfs:comment xml:lang="en">A sensor is a device that measures a physical quantity and converts it into a signal which can be read by an observer or by an instrument.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">http://ifcowl.openbimstandards.org/IFC4_ADD2/index.html#IfcSensor</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">IfcSensor</rdfs:label>
    </owl:Class>



    <!-- http://ifcowl.openbimstandards.org/IFC4_ADD2/index.html#IfcTank -->

    <owl:Class rdf:about="http://ifcowl.openbimstandards.org/IFC4_ADD2/index.html#IfcTank">
        <owl:equivalentClass rdf:resource="http://www.buildingsmart-tech.org/ifcOWL/IFC2X3_Final#IfcTankType"/>
        <rdfs:subClassOf rdf:resource="http://www.buildingsmart-tech.org/ifcOWL/IFC2X3_Final#IfcFlowStorageDevice"/>
        <rdfs:comment xml:lang="en">A tank is a vessel or container in which a fluid or gas is stored for later use.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">http://ifcowl.openbimstandards.org/IFC4_ADD2/index.html#IfcTank</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">IfcTank</rdfs:label>
    </owl:Class>



    <!-- http://purl.oclc.org/NET/ssnx/ssn#Sensor -->

    <owl:Class rdf:about="http://purl.oclc.org/NET/ssnx/ssn#Sensor">
        <rdfs:subClassOf rdf:resource="http://bemserver.org/appliance##Monitoring"/>
        <rdfs:comment xml:lang="en">A sensor can do (implements) sensing: that is, a sensor is any entity that can follow a sensing method and thus observe some Property of a FeatureOfInterest.  Sensors may be physical devices, computational methods, a laboratory setup with a person following a method, or any other thing that can follow a Sensing Method to observe a Property.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/Sensor"/>
        <rdfs:isDefinedBy xml:lang="en">http://purl.oclc.org/NET/ssnx/ssn#Sensor</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Sensor</rdfs:label>
    </owl:Class>



    <!-- http://www.buildingsmart-tech.org/ifcOWL/IFC2X3_Final#IfcActuatorType -->

    <owl:Class rdf:about="http://www.buildingsmart-tech.org/ifcOWL/IFC2X3_Final#IfcActuatorType">
        <rdfs:subClassOf rdf:resource="http://bemserver.org/appliance##Monitoring"/>
        <rdfs:comment xml:lang="en">IfcActuatorType is defined in IFC2X3. We made it equivalent to IfcActuator defined in IFC4.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">http://www.buildingsmart-tech.org/ifcOWL/IFC2X3_Final#IfcActuatorType</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">IfcActuatorType</rdfs:label>
    </owl:Class>



    <!-- http://www.buildingsmart-tech.org/ifcOWL/IFC2X3_Final#IfcAlarmType -->

    <owl:Class rdf:about="http://www.buildingsmart-tech.org/ifcOWL/IFC2X3_Final#IfcAlarmType">
        <rdfs:subClassOf rdf:resource="http://bemserver.org/appliance##Monitoring"/>
        <rdfs:comment xml:lang="en">IfcAlarmType is defined in IFC2X3. We made it equivalent to IfcAlarm defined in IFC4.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">http://www.buildingsmart-tech.org/ifcOWL/IFC2X3_Final#IfcAlarmType</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">IfcAlarmType</rdfs:label>
